{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/pages/index.html"
    }
  ],
  "routes": [
    {
      "src": "/src/pages/(.*)",
      "dest": "/src/pages/$1"
    },
    {
      "src": "/src/css/(.*)",
      "dest": "/src/css/$1"
    },
    {
      "src": "/src/js/(.*)",
      "dest": "/src/js/$1"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    }
  ]
} 